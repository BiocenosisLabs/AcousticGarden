{
  "name": "acoustic-garden",
  "version": "1.0.0",
  "description": "Monorepo for acoustic gardens, a SNI hackathon project 2022",
  "main": "index.js",
  "scripts": {
    "start": "npm run server:watch",
    "build": "babel ./src --out-dir ./build --copy-files",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:new-migration": "./node_modules/.bin/sequelize-cli migration:generate --name",
    "server:build": "babel ./src/server --out-dir ./build/server",
    "server:start": "cross-env NODE_ENV=production node ./build/server/index.js",
    "server:watch": "nodemon --watch ./src/server --exec cross-env NODE_ENV=development babel-node ./src/server/index.js --source-maps inline"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@react-three/drei": "^8.8.1",
    "@tailwindcss/typography": "^0.5.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "celebrate": "^14.0.0",
    "compression": "^1.7.4",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "http-status": "^1.5.0",
    "leaflet": "^1.7.1",
    "leva": "^0.9.18",
    "method-override": "^3.0.0",
    "mime": "^3.0.0",
    "multer": "^1.4.4",
    "path": "^0.12.7",
    "pg": "^8.6.0",
    "react-leaflet-canvas-overlay": "^0.1.3",
    "react-three-fiber": "^6.0.13",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "three": "^0.137.5"
  }
}
